<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".RootAppointmentActivity">

    <TextView
        android:id="@+id/singintxt14"
        android:layout_width="410dp"
        android:layout_height="130dp"
        android:background="#F3F3F3"
        android:textAlignment="gravity"
        android:textColor="@color/black"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.026" />

    <TextView
        android:id="@+id/singintxt5"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:fontFamily="@font/poppinslight"
        android:text="Detail TEC Administrador"
        android:textAlignment="gravity"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.586"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.129" />

    <TextView
        android:id="@+id/singintxt4"
        android:layout_width="195dp"
        android:layout_height="40dp"
        android:text="Root User 1"
        android:textAlignment="gravity"
        android:textColor="@color/black"
        android:textSize="28sp"
        android:textStyle="bold"
        android:fontFamily="@font/poppinssemibold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.625"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.057" />

    <TextView
        android:id="@+id/singintxt3"
        android:layout_width="413dp"
        android:layout_height="14dp"
        android:background="#223F71"
        android:textAlignment="gravity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.203" />

    <TextView
        android:id="@+id/singintxt2"
        android:layout_width="413dp"
        android:layout_height="14dp"
        android:background="#223F71"
        android:textAlignment="gravity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.002" />

    <ImageView
        android:id="@+id/imageView2"

        android:layout_width="416dp"
        android:layout_height="559dp"
        android:layout_marginTop="176dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/lavacar9"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/singintxt"
        android:layout_width="416dp"
        android:layout_height="36dp"
        android:background="@color/black"
        android:text="Ha ingresado como usuario Administrador, por lo tanto, se le han otorgado los permisos correspondientes."
        android:textAlignment="gravity"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.23" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="94dp"
        android:layout_height="85dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.085"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.062"
        app:srcCompat="@drawable/rootuserpic" />

    <TextView
        android:id="@+id/singintxt6"
        android:layout_width="434dp"
        android:layout_height="33dp"
        android:background="@color/black"
        android:fontFamily="@font/poppinslight"
        android:text="Asignando una Cita"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.237" />


    <LinearLayout
        android:id="@+id/linear_layout"
        android:layout_width="220dp"
        android:layout_height="129dp"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.524">

        <TextView
            android:id="@+id/txtView"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@drawable/custom_edittext3"
            android:fontFamily="@font/poppinssemibold"
            android:text="Sucursal"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="18dp" />

        <Spinner
            android:id="@+id/branchSpinnerRoot"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/txtView"
            android:background="@drawable/custom_edittext"
            android:fontFamily="@font/poppinslight"
            android:textAlignment="center"
            android:textSize="16dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear_layout2"
        android:layout_width="220dp"
        android:layout_height="129dp"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.692">

        <TextView
            android:id="@+id/txtView2"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@drawable/custom_edittext3"
            android:text="Tipo de lavado"
            android:textAlignment="center"
            android:fontFamily="@font/poppinssemibold"
            android:textColor="@color/white"
            android:textSize="18dp" />

        <Spinner
            android:id="@+id/serviceSpinnerRoot"
            android:layout_width="match_parent"
            android:fontFamily="@font/poppinslight"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/txtView"
            android:background="@drawable/custom_edittext"
            android:textAlignment="center"
            android:textSize="16dp" />

    </LinearLayout>

    <EditText
        android:id="@+id/plateEditTextRoot"
        android:layout_width="220dp"
        android:layout_height="48dp"
        android:background="@drawable/custom_edittext"
        android:fontFamily="@font/poppinslight"

        android:hint="Numero de Placa"
        android:inputType="number"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.799" />

    <EditText
        android:id="@+id/usernameAppointmentEditTextRoot"
        android:layout_width="220dp"
        android:layout_height="48dp"
        android:background="@drawable/custom_edittext"
        android:fontFamily="@font/poppinslight"
        android:hint="Usuario del cliente"
        android:inputType="text"

        android:textAlignment="center"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.39"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/backAppointmentButtonRoot"
        android:layout_width="wrap_content"
        android:layout_height="53dp"
        android:layout_marginTop="321dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/custom_buttons5"
        app:backgroundTint="@null"
        android:text="VOLVER"
        android:fontFamily="@font/poppinssemibold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.038"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/passwordLoginEditText"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/appointmentAcceptButtonRoot"
        android:layout_width="208dp"
        android:layout_height="53dp"
        android:background="@drawable/custom_buttons4"
        android:text="FINALIZAR"
        app:backgroundTint="@null"
        android:fontFamily="@font/poppinssemibold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.931"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976"
        app:strokeColor="#FFFFFF" />

</androidx.constraintlayout.widget.ConstraintLayout>