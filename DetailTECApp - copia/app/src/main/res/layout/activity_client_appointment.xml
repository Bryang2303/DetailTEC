<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ClientAppointmentActivity">

    <TextView
        android:id="@+id/singintxt14"
        android:layout_width="410dp"
        android:layout_height="126dp"
        android:background="#F3F3F3"
        android:textAlignment="gravity"
        android:textColor="@color/black"
        android:textSize="28sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.026" />

    <TextView
        android:id="@+id/singintxt5"
        android:layout_width="178dp"
        android:layout_height="35dp"
        android:text="Detail TEC Cliente"
        android:textAlignment="gravity"
        android:background="#F3F3F3"
        android:textColor="@color/black"
        android:fontFamily="@font/poppinslight"
        android:textSize="15sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.579"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.129" />

    <TextView
        android:id="@+id/usernameAppointmentTextViewClient"
        android:layout_width="195dp"
        android:layout_height="40dp"
        android:text="Client User 1"
        android:textAlignment="gravity"
        android:textColor="@color/black"
        android:textSize="28sp"
        android:fontFamily="@font/poppinssemibold"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.625"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.057" />

    <TextView
        android:id="@+id/singintxt3"
        android:layout_width="413dp"
        android:layout_height="14dp"
        android:background="#223F71"
        android:textAlignment="gravity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.002" />

    <TextView
        android:id="@+id/singintxt2"
        android:layout_width="413dp"
        android:layout_height="14dp"
        android:background="#223F71"
        android:textAlignment="gravity"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.2" />

    <ImageView
        android:id="@+id/imageView2"

        android:layout_width="416dp"
        android:layout_height="559dp"
        android:layout_marginTop="176dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/lavacar7"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp" />


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="94dp"
        android:layout_height="85dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.078"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.064"
        app:srcCompat="@drawable/taller5profile" />

    <TextView
        android:id="@+id/singintxt7"
        android:layout_width="417dp"
        android:layout_height="45dp"
        android:background="#F3F3F3"
        android:fontFamily="@font/poppinslight"
        android:text="Asegurese de haber seleccionado correctamente, su \nfactura sera enviada al finalizar la cita"
        android:textAlignment="center"
        android:textColor="#A83434"
        android:textSize="13sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/singintxt6"
        android:layout_width="427dp"
        android:layout_height="32dp"
        android:background="@color/black"
        android:fontFamily="@font/poppinslight"
        android:text="Solicitando su Cita"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.521"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.228" />

    <LinearLayout
        android:id="@+id/linear_layout"
        android:layout_width="220dp"
        android:layout_height="129dp"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.377">

        <TextView
            android:id="@+id/txtView"
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:background="@drawable/custom_edittext3"
            android:fontFamily="@font/poppinssemibold"
            android:text="Seleccione la        Sucursal"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="18dp" />

        <Spinner
            android:id="@+id/branchSpinnerClient"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/txtView"
            android:background="@drawable/custom_edittext"
            android:fontFamily="@font/poppinslight"
            android:textAlignment="center"
            android:textSize="16dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linear_layout2"
        android:layout_width="220dp"
        android:layout_height="129dp"
        android:gravity="center"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.591">

        <TextView
            android:id="@+id/txtView2"
            android:layout_width="match_parent"
            android:layout_height="58dp"
            android:background="@drawable/custom_edittext3"
            android:fontFamily="@font/poppinssemibold"
            android:text="Seleccione el tipo de lavado"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="18dp" />

        <Spinner
            android:id="@+id/serviceSpinnerClient"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@id/txtView"
            android:background="@drawable/custom_edittext"
            android:fontFamily="@font/poppinslight"
            android:textAlignment="center"
            android:textSize="16dp" />

    </LinearLayout>

    <EditText
        android:id="@+id/plateEditTextClient"
        android:layout_width="220dp"
        android:layout_height="45dp"
        android:background="@drawable/custom_edittext"
        android:fontFamily="@font/poppinslight"
        android:hint="Numero de Placa"
        android:inputType="number"

        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.743" />

    <Button
        android:id="@+id/backAppointmentButtonClient"
        android:layout_width="wrap_content"
        android:layout_height="53dp"
        android:layout_marginTop="321dp"
        android:layout_marginBottom="68dp"
        android:background="@drawable/custom_buttons5"
        android:fontFamily="@font/poppinssemibold"
        android:text="VOLVER"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.05"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/passwordLoginEditText"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/appointmentAcceptButtonClient"
        android:layout_width="208dp"
        android:layout_height="53dp"
        android:background="@drawable/custom_buttons4"
        android:fontFamily="@font/poppinssemibold"
        android:text="FINALIZAR"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.921"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.899"
        app:strokeColor="#FFFFFF" />


</androidx.constraintlayout.widget.ConstraintLayout>